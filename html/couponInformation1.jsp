/**
 * 用户注册
 */
function regist(){
	console.info('registLoading = ' + registLoading + ',usernameExsit=' + usernameExsit);
	if(registLoading){
		return false;
	}
	registLoading = true;
	console.info('set registLoading = ' + registLoading);
	var username = $("#username4regist").val();
	if(trim(username) == ''){
		$('#alertSpan4regist').html('请输入昵称');
		$("#username4regist").focus();
		registLoading = false;
		return ;
	} else if(usernameExsit){
		$('#alertSpan4regist').html('昵称已经存在,请更换昵称');
		$("#username4regist").focus();
		registLoading = false;
		console.info('set registLoading = ' + registLoading);
		return ;
	}
	var email = $("#email4regist").val();
	if(trim(email) == ''){
		$('#alertSpan4regist').html('请输入邮箱地址');
		$("#email4regist").focus();
		registLoading = false;
		return ;
	} else if(emailExsit){
		$('#alertSpan4regist').html('邮箱地址已经存在,请更换邮箱地址');
		$("#email4regist").focus();
		registLoading = false;
		console.info('set registLoading = ' + registLoading);
		return ;
	}
	var password = $("#password4regist").val();
	if(trim(password) == ''){
		$('#alertSpan4regist').html('请输入密码');
		$("#password4regist").focus();
		registLoading = false;
		return ;
	}
	$('#registLoadingImg').show();
	var userInfo = {'username':username , 'email':email , 'password':password , 'userType':1 , 'active':1};
	$.ajax({
        url : "/regist.do",
        method : 'POST',
        data : userInfo,
        dataType : "json",
        async : true,
        success:function(response){
			$('#alertSpan4regist').html(response.message);
			if(response.success){
				location.href = '/registSuccess.do';
			}
			$('#registLoadingImg').hide();
			registLoading = false;
			console.info('set registLoading = ' + registLoading);
        },
        error:function(){
        	$('#alertSpan4regist').html('注册失败,请重试');
        	$('#registLoadingImg').hide();
        	registLoading = false;
        	console.info('set registLoading = ' + registLoading);
        }
    });
}

/**
 * 用户登录验证
 */
function login(){
	$('#alertSpan4login').html('');
	if(loginLoading){
		return false;
	}
	loginLoading = true;
	var username = $("#username").val();
	if(trim(username) == ''){
		$('#alertSpan4login').html('请输入邮箱地址或者昵称');
		$("#username").focus();
		loginLoading = false;
		return false;
	}
	var password = $("#password").val();
	if(trim(password) == ''){
		$('#alertSpan4login').html('请输入密码');
		$("#password").focus();
		loginLoading = false;
		return false;
	}
	$('#loginLoadingImg').show();
	var userInfo = {};
	userInfo.username = username;
	userInfo.password = password;
	try{
		$.ajax({
	        url : "/login.do",
	        method : 'POST',
	        data : userInfo,
	        dataType : "json",
	        async : true,
	        success:function(response){
	        	$('#alertSpan4login').html(response.message);
	            if(response.success){
	            	location.reload();
	            }
	            $('#loginLoadingImg').hide();
	            loginLoading = false;
	            

	        },
	        error:function(XMLHttpRequest, textStatus, errorThrown){
		        alert(errorThrown);
	        	$('#alertSpan4login').html('登录失败,请重试');
	        	$('#loginLoadingImg').hide();
	        	loginLoading = false;
	        }
	    })
	}catch(e){
		$('#alertSpan4login').html('登录失败,请重试');
		$('#loginLoadingImg').hide();
		loginLoading = false;

	}
	
}

/**
 * 验证昵称是否存在
 */
function checkName(username , alertSpan , _info , callback){
	if(trim(username) == ''){
		return false;
	}
	try{
		$.ajax({
	        url : "/checkName.do",
	        method : 'POST',
	        data : {username : username},
	        dataType : "json",
	        async : true,
	        success:function(response){
	            if(response.exsit){
	            	usernameExsit = true;
                    if(_info){
                        $('#'+alertSpan).html(_info);
                    } else {
                        $('#'+alertSpan).html('昵称已经存在,请更换昵称');
                    }

	            } else {
	            	usernameExsit = false;
                    $('#'+alertSpan).html('');
		        }
                callback(usernameExsit);
	        },
	        error:function(XMLHttpRequest, textStatus, errorThrown){
	        	usernameExsit = false;
                callback(usernameExsit);
	        }
	    })
	}catch(e){
		usernameExsit = false;
        callback(usernameExsit);
	}
}

/**
 * 验证邮箱地址是否重复
 */
function checkEmail(email , alertSpan , _info , callback){
	if(trim(email) == ''){
		return false;
	}
	try{
		$.ajax({
	        url : "/checkEmail.do",
	        method : 'POST',
	        data : {email : email},
	        dataType : "json",
	        async : true,
	        success:function(response){
	            if(response.exsit){
	            	emailExsit = true;
                    if(_info){
                        $('#'+alertSpan).html(_info);
                    } else {
                        $('#'+alertSpan).html('邮箱地址已经存在,请更换邮箱地址');
                    }

	            } else {
	            	emailExsit = false;
                    $('#'+alertSpan).html('');
		        }
                callback(emailExsit);
	        },
	        error:function(XMLHttpRequest, textStatus, errorThrown){
	        	emailExsit = false;
                callback(emailExsit);
	        }
	    })
	}catch(e){
		emailExsit = false;
        callback(emailExsit);
	}
}

